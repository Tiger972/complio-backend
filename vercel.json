{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-license",
      "dest": "api/generate_license.py"
    },
    {
      "src": "/api/validate-license",
      "dest": "api/validate_license.py",
      "methods": ["POST"]
    }
  ]
}
